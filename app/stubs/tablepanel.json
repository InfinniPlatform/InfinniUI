  "Items": [
    {
      "~DataGrid": {
        "Items": {
          "Source": "BloodGroupDataSource",
          "Property": ""
        },
        "Columns": [
          {
            "Header": "Email",
            "CellProperty": "Email"
          }
        ]
      }
    },
    {
      "~ListBox": {
        "Items": {
          "Source": "BloodGroupDataSource",
          "Property": ""
        },
        "ItemTemplate": {
          "StackPanel": {
            "Items": [{
              "Label": {
                "Value": {
                  "Source": "BloodGroupDataSource",
                  "Property": "$.Email"
                }
              }
            }]
          }
        }
      }
    }, {
      "TablePanel": {
        "Items": {
          "Source": "BloodGroupDataSource",
          "Property": ""
        },
        "ItemTemplate": {
          "Row": {
            "Items": [{
              "Cell": {
                "ColumnSpan": 9,
                "Items": [{
                  "TextBox": {
                    "Value": {
                      "Source": "BloodGroupDataSource",
                      "Property": "$.FirstName"
                    }
                  }
                }]
              }
            },{
              "Cell": {
                "ColumnSpan": 3,
                "Items": [{
                  "Button": {
                    "Name": "FeeButton",
                    "Image": "rub",
                    "Style": "button-link green-icon",
                    "Text": {
                      "Source": "BloodGroupDataSource",
                      "Property": "$.Email"
                    },
                    "OnClick": {
                      "Name": "PaymentButtonOnClick"
                    }
                  }
                }]
              }
            }]
          }
        }
      }
    },
    {
      "~ComboBox": {
        "Items": {
          "Source": "BloodGroupDataSource",
          "Property": ""
        },
        "ItemFormat": "{Email} ({FirstName})",
        "ValueFormat": "{Email}"
      }
    }
  ],
  "Scripts": [
    {
      "Name": "PaymentButtonOnClick",
      "Body": "console.log('PaymentButtonOnClick');"
    },
    {
