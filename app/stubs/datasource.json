{
  "Text": "Пациенты",
  "DataSources": [
    {
      "ObjectDataSource": {
        "Name": "BloodGroupDataSource",
        "Items": [
          {
            "Id": 1,
            "DisplayName": "I"
          },
          {
            "Id": 2,
            "DisplayName": "II"
          },
          {
            "Id": 3,
            "DisplayName": "III"
          },
          {
            "Id": 4,
            "DisplayName": "IV"
          }
        ]
      }
    }
  ],
  "LayoutPanel": {
    "StackPanel": {
      "Name": "MainViewPanel",
      "Items": [
        {
          "RadioGroup": {
            "Orientation": "Vertical",
            "Items": {
              "PropertyBinding": {
                "DataSource": "BloodGroupDataSource",
                "Property": ""
              }
            },
            "ItemTemplate": {
              "StackPanel": {
                "Orientation": "Vertical",
                "Items": [
                  {
                    "Label": {
                      "Value": {
                        "PropertyBinding": {
                          "DataSource": "BloodGroupDataSource",
                          "Property": "$.DisplayName"
                        }
                      }
                    }
                  },
                  {
                    "Label": {
                      "Value": {
                        "PropertyBinding": {
                          "DataSource": "ItemsDataSource",
                          "Property": "$.DisplayName"
                        }
                      }
                    }
                  }
                ]
              }
            },
            "~ItemFormat": {
              "ObjectFormat": {
                "Format": "{Code}: {DisplayName}"
              }
            },
            "DisplayProperty": "DisplayName",
            "~ValueProperty": "Code",
            "ValueProperty": {
              "Id": "Code",
              "DisplayName": "DisplayName"
            },
            "Value": {
              "PropertyBinding": {
                "DataSource": "MainDataSource",
                "Property": "Radio"
              }
            }
          }
        }
      ]
    }
  },
  "OnLoaded": {
    "Name": "OnLoaded"
  },
  "Scripts": [
    {
      "Name": "OnLoaded",
      "Body": "console.log('OnLoaded'); console.log(arguments); console.log(context.DataSources.BloodGroupDataSource.getDataItems());return false;"
    }
  ]
}